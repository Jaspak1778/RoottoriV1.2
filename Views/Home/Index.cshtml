﻿@model IEnumerable<RoottoriV1._2.Models.Roottorit>
﻿

@{
    ViewBag.Title = "Home Page";
}
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">


    <title>@ViewBag.Title  ROOTTORI</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <!--Yllä olevaan ei tarvitse koskea-->
</head>

<main>
    
    <ul class="nav nav-pills">
        <li class="nav-item dropdown">
            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">Työkalut</a>
            <div class="dropdown-menu">

                <a class="dropdown-item" href="~/KirjastoTyokalut/Mazak400">Mazak400</a>
                <a class="dropdown-item" href="~/KirjastoTyokalut/Mazaki500">Mazaki500</a>
                <a class="dropdown-item" href="~/KirjastoTyokalut/Index">Kaikki työkalut</a>

            </div>
        </li>


        <li class="nav-item dropdown">
            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">Asetuskortti</a>
            <div class="dropdown-menu">
                <a class="dropdown-item" href="~/Roottorit/E6R">Malli E6R</a>
                <a class="dropdown-item" href="~/Roottorit/E25R">Malli E25R</a>
                <a class="dropdown-item" href="~/Roottorit/E25Ri">Malli E25Ri</a>
            </div>
        </li>

        <li class="nav-item dropdown">
            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">Suunnittelijan näkymä</a>
            <div class="dropdown-menu">
                <a class="dropdown-item" href="~/Roottorit/Index">Mallit</a>
                <a class="dropdown-item" href=#>Item2</a>
                <a class="dropdown-item" href=#>Item3</a>
            </div>
        </li>


    </ul>
</main>

@section Scripts{
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
    <script>

        var dropdownToggleList = [].slice.call(document.querySelectorAll('.dropdown-toggle'));
        dropdownToggleList.map(function (dropdownToggleEl) {
            return new bootstrap.Dropdown(dropdownToggleEl);
        });
    </script>

}