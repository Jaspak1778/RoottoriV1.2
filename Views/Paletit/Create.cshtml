@model RoottoriV1._2.Models.Paletit

@{
    ViewBag.Title = "Create";
}

<h2>Luo Paletti Tietue</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <h4>Paletit</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            @Html.LabelFor(model => model.Paletti, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.Paletti, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Paletti, "", new { @class = "text-danger" })
            </div>
        </div>
        <br />
        <div class="container">
            <div class="row">
                <div class="col-auto">
                    <a class="btn button2" data-toggle="tooltip" data-placement="bottom" title="Takaisin" href="javascript:void(0);" onclick="history.go(-1);">Takaisin</a>
                </div>
                <div class="col-auto">
                    <button class="btn button3" data-toggle="tooltip" data-placement="bottom" title="Talenna" type="submit">Talleta</button>
                </div>
            </div>
        </div>


    </div>
}


